<%= simple_form_for(@site) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :domain %>
    <%= f.input :title %>
    <%= f.input :keywords %>
    <%= f.input :description %>
    <%= f.input :is_processed %>
    <%= f.input :note %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
